{
    "manifest_version": 3,
    "name": "OI Jumper",
    "version": "1.0",
    "description": "Jump to whatever Online Judge by Problem ID.",
    "icons": {
        "16": "icon-16.png",
        "48": "icon-48.png",
        "128": "icon-128.png"
    },
    "action": {
        "default_icon": "icon.png",
        "default_title": "OI Jumper"
    },
    "chrome_url_overrides": {
        "newtab": "index.html"
    },
    "permissions": []
}